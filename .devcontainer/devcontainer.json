{
    "name": "codespaces-laravel",
    "dockerComposeFile": ["docker-compose.yml"],
    "workspaceFolder": "/workspace",
    "service": "app",
    "shutdownAction": "stopCompose",
    "extensions": [
        "editorconfig.editorconfig",
        "ryannaddy.laravel-artisan",
        "amiralizadeh9480.laravel-extra-intellisense",
        "stef-k.laravel-goto-controller",
        "codingyu.laravel-goto-view",
        "mikestead.dotenv",
        "eg2.tslint",
        "christian-kohler.path-intellisense",
        "esbenp.prettier-vscode",
        "CoenraadS.bracket-pair-colorizer",
        "felixfbecker.php-debug",
        "bmewburn.vscode-intelephense-client",
        "felixfbecker.php-curl",
        "felixfbecker.php-json",
        "bmewburn.vscode-intelephense-client",
        "ikappas.php-mysql",
        "felixfbecker.php-bcmath",
        "felixfbecker.php-mbstring",
        "felixfbecker.php-xml",
        "felixfbecker.php-zip",
        "felixfbecker.php-intl"
    ],
    "forwardPorts": [80],
    "postCreateCommand": "cp .env.example .env && composer install && php artisan key:generate && yarn install && yarn run development",
    "portsAttributes": {
        "80": {
            "label": "HTTP"
        }
    },
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    }
}
